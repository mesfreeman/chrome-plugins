{
    "manifest_version": 2,
    "name": "永远点不到的搜索按钮",
    "version": "1.0",
    "description": "让你永远也点不到Google的搜索按钮",
    "icons": {
        "16": "images/kulou.png",
        "48": "images/kulou.png",
        "128": "images/kulou.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "/images/kulou.png",
            "48": "/images/kulou.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["*://www.google.com.hk/"],
            "js": ["js/not_touch.js"]
        }
    ]
}